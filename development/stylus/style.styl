@font-face
    font-family 'Roboto'
    font-style normal
    font-weight 400
    src local('Roboto'), local('Roboto-Regular'), url(../fonts/roboto.woff2) format('woff2')
    unicode-range U+0000-00FF, U+0131, U+0152-0153, U+02C6, U+02DA, U+02DC, U+2000-206F, U+2074, U+20AC, U+2212, U+2215

html, body, .h100
    height 100%
    background #6f6d6b
    font-family 'Roboto', sans-serif

.main
    background #808080
    padding 20px
    font-size 18px
    box-shadow 0 0 20px

small
    font-size 13px

h3
	font-size 28px
	
.center
	text-align center
	
.input1
    background-color #504d4d
    color #000000
	font-family 'Roboto', sans-serif
    font-size 24px
    width 618px
    height 100px
    padding 0 8px
    border none
    text-align center

.input2
    margin-top 15px
    font-size 18px
    color #000000
    padding 0 15px
    height 50px
    cursor pointer
    background-color #504d4d
    border none

#my-slider
    margin-top 15px
    border none

label
    width 148px
    display inline-block
    cursor pointer
    line-height 48px
    background-color #504d4d

#boxes
    margin-top 15px
    padding 1px 0 1px 0

.dragdealer
    position relative
    height 48px
    background #383737
    .handle
        position absolute
        cursor pointer
    .red-bar
        width 100px
        height 48px
        background #504d4d
        color #000
        line-height 48px
        text-align center
    .disabled
        background #898989
 
  ::-webkit-scrollbar
  z-index 30
  width 10px
  height 0
  y-index hidden

::-webkit-scrollbar-track
  background-color #111
  border-radius 0
  box-shadow none
  border 0

::-webkit-scrollbar-thumb
  background-color #2b2e33
  border-radius 0
  box-shadow none
  border 0
  &:hover
    background-color transparent